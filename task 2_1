class Deteils:
    def __init__(self,name,age,title):
        self.name=name
        self.age=age
        self.title=title

    def cheek_name(self):
        listcheek = []
        listcheek[:0] = self.name
        return listcheek

    listname = cheek_name(name) #ان هنا في هذا السؤال يوجد خطا لم استطع حله 
    if listname[0] == listname[-1]:
        print('the first and fainal leeter it is seam')

    def cheek_age (self):
        if self.age >30:
            return 'old'
        elif 20< self.age <30:
            return 'young'
        elif self.age<20:
            return 'illagel'
        else:
            return 'error age'

